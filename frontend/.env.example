# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3141
NEXTAUTH_SECRET=your-long-random-secret-key-at-least-32-characters

# Discord OAuth
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret

# PayPal Configuration
NEXT_PUBLIC_PAYPAL_CLIENT_ID=your-paypal-client-id
PAYPAL_CLIENT_SECRET=your-paypal-client-secret

# PayPal Subscription Plan IDs (create these in PayPal Dashboard)
NEXT_PUBLIC_PAYPAL_PLAN_ID_MONTHLY=your-monthly-plan-id
NEXT_PUBLIC_PAYPAL_PLAN_ID_YEARLY=your-yearly-plan-id

# Environment (development/production)
NODE_ENV=development