{
  "name": "stoffel-bot-backend",
  "version": "0.1.0",
  "description": "Backend for Stoffel Discord Bot",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop stoffel-bot-backend",
    "pm2:restart": "pm2 restart stoffel-bot-backend",
    "pm2:logs": "pm2 logs stoffel-bot-backend",
    "pm2:dev": "pm2 start ecosystem.config.js --env development --watch"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.7",
    "@discordjs/opus": "^0.10.0",
    "@distube/ytdl-core": "^4.16.12",
    "cors": "^2.8.5",
    "discord-player": "^6.6.6",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "pm2": "^6.0.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
