{
  "name": "stoffel-bot-backend",
  "version": "0.1.0",
  "description": "Backend for Stoffel Discord Bot",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop stoffel-bot-backend",
    "pm2:restart": "pm2 restart stoffel-bot-backend",
    "pm2:logs": "pm2 logs stoffel-bot-backend",
    "pm2:dev": "pm2 start ecosystem.config.js --env development --watch"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "discord.js": "^14.13.0",
    "pm2": "^6.0.9"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}